{
	"manifest_version": 2,
	"name": "Remove intra 42 red button",
	"description": "Remove the big ugly red button on the intranet pages",
	"version": "1",
	"content_scripts": [
		{
		  "matches": ["https://*.intra.42.fr/*"],
		  "js": ["intra.js"]
		}
	  ],
	"permissions": ["tabs", "activeTab"],
	"browser_action": {
		"default_icon": "red_button.png",
		"default_title": "Remove intra 42 red button"
	}
}
